{
  "name": "filtros-lubs-rios",
  "version": "0.0.0",
  "scripts": {
      "build": "ng build",
      "electron": "electron ./electron/main.js",
      "start": "npm run build && npm run electron",

      "start:ng": "ng serve",
      "start:electron": "cross-env NODE_ENV=development electronmon ./electron/main.js",
      "dev": "concurrently -k \"npm run start:ng\" \"wait-on http://localhost:4200 && npm run start:electron\""
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/router": "^20.0.0",
    "apexcharts": "^4.7.0",
    "chart.js": "^4.5.0",
    "mssql": "^11.0.1",
    "ng-apexcharts": "^1.16.0",
    "ng2-charts": "^8.0.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.22.3",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.0.4",
    "@angular/cli": "^20.0.4",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.1.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electronmon": "^2.0.3",
    "jasmine-core": "~5.7.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.8.2",
    "wait-on": "^8.0.4"
  }
}
